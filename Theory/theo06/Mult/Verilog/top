    %load/vec4 v0x5607be45b830_0;
    %addi 2, 0, 32;
    %muli 2, 0, 32;
    %store/vec4 v0x5607be45b720_0, 0, 32;
    %delay 10000, 0;
    %load/vec4 v0x5607be45b830_0;
    %addi 2, 0, 32;
    %pad/s 16;
    %store/vec4 v0x5607be45b4b0_0, 0, 16;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
