    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5607be45b4b0_0, 0, 16;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5607be45b830_0, 0, 32;
T_1.0 ;
